{
    "name": "message",
    "version": "0.1.0",
    "private": true,
    "main": "bootstrap.js",
    "scripts": {
        "start": "$(npm bin)/electron .",
        "for-mac": "$(npm bin)/electron-packager . Message --platform=darwin --arch=x64 --icon=./src/public/logo.icns --app-version=0.1.0 --app-bundle-id=no.message --helper-bundle-id=no.message.helper --out ./dist --overwrite",
        "for-windows": "$(npm bin)/electron-packager . Message --out ./dist --overwrite --platform=win32 --arch=ia32 --app-version=0.1.0 --icon=./src/public/logo.ico --prune=true",
        "compression": "maximum"
    },
    "engines": {
        "node": ">=8.9.3"
    },
    "devDependencies": {
        "electron": "^10.1.3",
        "electron-packager": "^15.1.0"
    },
    "dependencies": {
        "babel-core": "^6.26.3",
        "babel-preset-es2015": "^6.24.1",
        "electron-compile": "^6.4.4",
        "electron-compilers": "^5.9.0",
        "electron-storage": "^1.0.7"
    }
}