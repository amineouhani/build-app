{
    "name": "Message-app",
    "version": "0.1.0",
    "private": true,
    "main": "bootstrap.js",
    "scripts": {
        "start": "$(npm bin)/electron .",
        "package-macos": "$(npm bin)/electron-packager . Message --platform=darwin --arch=x64 --icon=./src/public/logo.icns --app-version=0.1.0 --app-bundle-id=no.messageApp --helper-bundle-id=no.messageApp.helper --out ./dist --overwrite",
        "package-windows": "$(npm bin)/electron-packager . Message --out ./dist --overwrite --platform=win32 --arch=ia32 --app-version=0.1.0 --icon=./src/public/logo.ico --prune=true",
        "package-linux": "$(npm bin)/electron-packager . Message --out ./dist --overwrite --platform=linux --arch=x64 --icon=./src/public/logo.png --prune=true --app-version=0.1.0"
    },
    "engines": {
        "node": ">=8.9.3"
    },
    "devDependencies": {
        "electron": "^2.0.5",
        "electron-packager": "^12.1.0"
    },
    "dependencies": {
        "babel-core": "^6.25.0",
        "babel-preset-es2015": "^6.24.1",
        "electron-compile": "^6.4.1",
        "electron-compilers": "^5.9.0"
    }
}